  <script>
    let currentDimension = 'narcissism';

    // 페이지 로드 시 인증 확인 및 초기화
    document.addEventListener('DOMContentLoaded', function() {
      // 인증 확인
      const savedAdmin = localStorage.getItem('currentAdmin');
      if (!savedAdmin) {
        alert('로그인이 필요합니다.');
        window.location.href = 'index.html';
        return;
      }
      const currentAdmin = JSON.parse(savedAdmin);
      document.getElementById('adminWelcome').textContent = `${currentAdmin.name}님 환영합니다`;
      
      // 초기화
      updateDateTime();
      setInterval(updateDateTime, 60000);
      loadFeedbackData();
      setupEventListeners();
    });

    // 이벤트 리스너 설정
    function setupEventListeners() {
      // 폼 제출 이벤트
      document.getElementById('narcissismFeedbackForm').addEventListener('submit', function(e) {
        e.preventDefault();
        saveDimensionFeedback('narcissism');
      });

      document.getElementById('machiavellianismFeedbackForm').addEventListener('submit', function(e) {
        e.preventDefault();
        saveDimensionFeedback('machiavellianism');
      });

      document.getElementById('psychopathyFeedbackForm').addEventListener('submit', function(e) {
        e.preventDefault();
        saveDimensionFeedback('psychopathy');
      });

      // 실시간 미리보기 이벤트
      setupRealtimePreview();
    }

    // 실시간 미리보기 설정
    function setupRealtimePreview() {
      const dimensions = ['narcissism', 'machiavellianism', 'psychopathy'];
      const fields = ['name', 'shortDesc', 'description', 'positiveAspects', 'risks', 'developmentTips'];
      
      dimensions.forEach(dimension => {
        fields.forEach(field => {
          const element = document.getElementById(`${dimension}_${field}`);
          if (element) {
            element.addEventListener('input', () => updatePreview(dimension));
          }
        });
      });

      // 종합 해석 미리보기
      const overallFields = ['veryLow_title', 'veryLow_description', 'veryLow_advice',
                            'low_title', 'low_description', 'low_advice',
                            'moderate_title', 'moderate_description', 'moderate_advice',
                            'high_title', 'high_description', 'high_advice'];
      
      overallFields.forEach(fieldId => {
        const element = document.getElementById(fieldId);
        if (element) {
          element.addEventListener('input', () => updateOverallPreview());
        }
      });
    }

    // 현재 시간 업데이트
    function updateDateTime() {
      const now = new Date();
      const dateTimeString = now.toLocaleString('ko-KR');
      document.getElementById('currentDateTime').textContent = dateTimeString;
    }

    // 로그아웃
    function logout() {
      if (confirm('로그아웃 하시겠습니까?')) {
        localStorage.removeItem('currentAdmin');
        window.location.href = 'index.html';
      }
    }

    // 피드백 데이터 로드
    function loadFeedbackData() {
      const customFeedback = JSON.parse(localStorage.getItem('customFeedback') || '{}');
      
      if (Object.keys(customFeedback).length === 0) {
        initializeDefaultFeedback();
        return;
      }
      
      // 각 차원별 데이터 로드
      loadDimensionFeedbackData('narcissism', customFeedback.narcissism);
      loadDimensionFeedbackData('machiavellianism', customFeedback.machiavellianism);
      loadDimensionFeedbackData('psychopathy', customFeedback.psychopathy);
      loadOverallInterpretationData(customFeedback.overall);
      
      // 초기 미리보기 업데이트
      updatePreview('narcissism');
    }

    // 기본 피드백 초기화
    function initializeDefaultFeedback() {
      const defaultFeedback = {
        narcissism: {
          name: "나르시시즘",
          shortDesc: "자기중심적 사고와 우월감",
          description: "자신에 대한 과도한 자신감과 우월감, 타인의 칭찬과 인정에 대한 강한 욕구를 나타냅니다.",
          positiveAspects: ["강한 자신감과 추진력", "도전적인 목표 설정", "카리스마 있는 리더십"],
          risks: ["팀원의 의견 무시", "현실 인식 부족", "비판에 대한 과민반응"],
          developmentTips: ["공감 능력 개발하기", "팀원의 피드백을 적극적으로 듣기", "겸손함 연습하기"]
        },
        machiavellianism: {
          name: "마키아벨리즘",
          shortDesc: "목적을 위한 수단 정당화",
          description: "목표 달성을 위해 수단과 방법을 가리지 않으며, 타인을 조작하거나 이용하는 경향을 나타냅니다.",
          positiveAspects: ["전략적 사고 능력", "협상력과 설득력", "현실적인 판단력"],
          risks: ["팀원과의 신뢰 관계 손상", "단기적 성과에 치중", "윤리적 딜레마 발생"],
          developmentTips: ["투명하고 정직한 커뮤니케이션", "윤리적 기준 설정하기", "장기적 관계 중시하기"]
        },
        psychopathy: {
          name: "사이코패시",
          shortDesc: "감정적 냉담함과 충동성",
          description: "타인에 대한 공감 부족, 감정적 냉담함, 그리고 충동적인 행동 경향을 나타냅니다.",
          positiveAspects: ["압박 상황에서의 냉정함", "빠른 의사결정 능력", "스트레스 저항력"],
          risks: ["팀원과의 감정적 단절", "충동적인 의사결정", "윤리적 판단력 부족"],
          developmentTips: ["공감 능력 개발하기", "팀원의 감정 상태 관찰하기", "의사결정 전 숙고하기"]
        },
        overall: {
          veryLow: {
            title: "건강한 리더십 성향",
            description: "매우 건강하고 균형 잡힌 리더십 성향을 보이고 있습니다.",
            advice: "현재의 균형 잡힌 리더십 스타일을 유지하면서, 지속적인 자기계발에 집중하세요."
          },
          low: {
            title: "안정적인 리더십 성향",
            description: "전반적으로 안정적이고 건강한 리더십 성향을 나타냅니다.",
            advice: "현재의 긍정적인 리더십 특성을 더욱 발전시키고, 팀과의 소통에 더욱 집중하세요."
          },
          moderate: {
            title: "주의가 필요한 리더십 성향",
            description: "일부 영역에서 주의깊은 자기성찰과 개선이 필요합니다.",
            advice: "자신의 리더십 스타일을 점검하고, 팀원들의 피드백을 적극적으로 수용하여 개선하세요."
          },
          high: {
            title: "개선이 시급한 리더십 성향",
            description: "리더십에서 잠재적 위험 요소가 높아 즉각적인 개선이 필요합니다.",
            advice: "전문적인 리더십 코칭이나 교육을 통해 건강한 리더십 스타일로 전환하는 것을 강력히 권장합니다."
          }
        }
      };
      
      localStorage.setItem('customFeedback', JSON.stringify(defaultFeedback));
      loadFeedbackData();
    }

    // 차원별 피드백 데이터 로드
    function loadDimensionFeedbackData(dimension, data) {
      if (!data) return;
      
      document.getElementById(`${dimension}_name`).value = data.name || '';
      document.getElementById(`${dimension}_shortDesc`).value = data.shortDesc || '';
      document.getElementById(`${dimension}_description`).value = data.description || '';
      document.getElementById(`${dimension}_positiveAspects`).value = data.positiveAspects ? data.positiveAspects.join('\n') : '';
      document.getElementById(`${dimension}_risks`).value = data.risks ? data.risks.join('\n') : '';
      document.getElementById(`${dimension}_developmentTips`).value = data.developmentTips ? data.developmentTips.join('\n') : '';
    }

    // 종합 해석 데이터 로드
    function loadOverallInterpretationData(data) {
      if (!data) return;
      
      const levels = ['veryLow', 'low', 'moderate', 'high'];
      levels.forEach(level => {
        if (data[level]) {
          document.getElementById(`${level}_title`).value = data[level].title || '';
          document.getElementById(`${level}_description`).value = data[level].description || '';
          document.getElementById(`${level}_advice`).value = data[level].advice || '';
        }
      });
    }

    // 차원 피드백 표시
    function showDimensionFeedback(dimension) {
      currentDimension = dimension;
      
      // 탭 활성화 상태 변경
      document.querySelectorAll('.dimension-tab').forEach(tab => {
        tab.classList.remove('active', 'border-blue-500', 'text-blue-600');
        tab.classList.add('border-transparent', 'text-gray-500');
      });
      
      const activeTab = document.querySelector(`[data-dimension="${dimension}"]`);
      activeTab.classList.add('active', 'border-blue-500', 'text-blue-600');
      activeTab.classList.remove('border-transparent', 'text-gray-500');
      
      // 콘텐츠 표시/숨김
      document.querySelectorAll('.dimension-content').forEach(content => {
        content.classList.add('hidden');
      });
      
      document.getElementById(`${dimension}Feedback`).classList.remove('hidden');
      
      // 미리보기 업데이트
      if (dimension === 'overall') {
        updateOverallPreview();
      } else {
        updatePreview(dimension);
      }
    }

    // 차원별 미리보기 업데이트
    function updatePreview(dimension) {
      const name = document.getElementById(`${dimension}_name`).value || '차원명';
      const shortDesc = document.getElementById(`${dimension}_shortDesc`).value || '간단 설명';
      const description = document.getElementById(`${dimension}_description`).value || '상세 설명을 입력해주세요';
      const positiveAspects = document.getElementById(`${dimension}_positiveAspects`).value.split('\n').filter(item => item.trim());
      const risks = document.getElementById(`${dimension}_risks`).value.split('\n').filter(item => item.trim());
      const developmentTips = document.getElementById(`${dimension}_developmentTips`).value.split('\n').filter(item => item.trim());
      
      const preview = document.getElementById(`${dimension}Preview`);
      const colorMap = {
        narcissism: 'red',
        machiavellianism: 'orange',
        psychopathy: 'teal'
      };
      const color = colorMap[dimension];
      
      preview.innerHTML = `
        <h6 class="font-semibold text-${color}-600 text-lg mb-2">${name}</h6>
        <p class="text-sm text-gray-600 mb-3">${shortDesc}</p>
        <p class="text-sm mb-4">${description}</p>
        
        <div class="grid grid-cols-1 gap-4">
          <div>
            <h6 class="font-medium text-green-700 mb-2">긍정적 측면</h6>
            <ul class="text-sm space-y-1">
              ${positiveAspects.length > 0 ? positiveAspects.map(aspect => `<li>• ${aspect}</li>`).join('') : '<li>내용을 입력해주세요</li>'}
            </ul>
          </div>
          
          <div>
            <h6 class="font-medium text-red-700 mb-2">주의사항</h6>
            <ul class="text-sm space-y-1">
              ${risks.length > 0 ? risks.map(risk => `<li>• ${risk}</li>`).join('') : '<li>내용을 입력해주세요</li>'}
            </ul>
          </div>
          
          <div>
            <h6 class="font-medium text-blue-700 mb-2">개발 팁</h6>
            <ul class="text-sm space-y-1">
              ${developmentTips.length > 0 ? developmentTips.map(tip => `<li>• ${tip}</li>`).join('') : '<li>내용을 입력해주세요</li>'}
            </ul>
          </div>
        </div>
      `;
    }

    // 종합 해석 미리보기 업데이트
    function updateOverallPreview() {
      const levels = [
        { id: 'veryLow', color: 'green' },
        { id: 'low', color: 'blue' },
        { id: 'moderate', color: 'yellow' },
        { id: 'high', color: 'red' }
      ];

      levels.forEach(level => {
        const title = document.getElementById(`${level.id}_title`).value || '제목을 입력해주세요';
        const description = document.getElementById(`${level.id}_description`).value || '설명을 입력해주세요';
        const advice = document.getElementById(`${level.id}_advice`).value || '권장사항을 입력해주세요';
        
        const preview = document.getElementById(`${level.id}_preview`);
        preview.innerHTML = `
          <h6 class="font-semibold text-${level.color}-600">${title}</h6>
          <p class="mt-1 text-sm">${description}</p>
          <p class="mt-2 text-xs text-${level.color}-700"><strong>권장사항:</strong> ${advice}</p>
        `;
      });
    }

    // 차원별 피드백 저장
    function saveDimensionFeedback(dimension) {
      const customFeedback = JSON.parse(localStorage.getItem('customFeedback') || '{}');
      
      const feedbackData = {
        name: document.getElementById(`${dimension}_name`).value,
        shortDesc: document.getElementById(`${dimension}_shortDesc`).value,
        description: document.getElementById(`${dimension}_description`).value,
        positiveAspects: document.getElementById(`${dimension}_positiveAspects`).value.split('\n').filter(item => item.trim()),
        risks: document.getElementById(`${dimension}_risks`).value.split('\n').filter(item => item.trim()),
        developmentTips: document.getElementById(`${dimension}_developmentTips`).value.split('\n').filter(item => item.trim())
      };
      
      customFeedback[dimension] = feedbackData;
      localStorage.setItem('customFeedback', JSON.stringify(customFeedback));
      
      alert(`${feedbackData.name} 피드백이 저장되었습니다.`);
    }

    // 종합 해석 저장
    function saveOverallInterpretations() {
      const customFeedback = JSON.parse(localStorage.getItem('customFeedback') || '{}');
      
      const overallData = {
        veryLow: {
          title: document.getElementById('veryLow_title').value,
          description: document.getElementById('veryLow_description').value,
          advice: document.getElementById('veryLow_advice').value
        },
        low: {
          title: document.getElementById('low_title').value,
          description: document.getElementById('low_description').value,
          advice: document.getElementById('low_advice').value
        },
        moderate: {
          title: document.getElementById('moderate_title').value,
          description: document.getElementById('moderate_description').value,
          advice: document.getElementById('moderate_advice').value
        },
        high: {
          title: document.getElementById('high_title').value,
          description: document.getElementById('high_description').value,
          advice: document.getElementById('high_advice').value
        }
      };
      
      customFeedback.overall = overallData;
      localStorage.setItem('customFeedback', JSON.stringify(customFeedback));
      
      alert('종합 해석이 모두 저장되었습니다.');
    }

    // 기본값으로 복원
    function resetToDefaults() {
      if (confirm('모든 피드백 내용을 기본값으로 복원하시겠습니까?\n현재 편집 중인 내용이 모두 사라집니다.')) {
        localStorage.removeItem('customFeedback');
        initializeDefaultFeedback();
        alert('기본값으로 복원되었습니다.');
      }
    }

    // 키보드 단축키
    document.addEventListener('keydown', function(e) {
      // Ctrl+S로 현재 차원 저장
      if (e.ctrlKey && e.key === 's') {
        e.preventDefault();
        if (currentDimension === 'overall') {
          saveOverallInterpretations();
        } else {
          saveDimensionFeedback(currentDimension);
        }
      }
      
      // 탭 키로 차원 전환
      if (e.key === 'Tab' && e.altKey) {
        e.preventDefault();
        const dimensions = ['narcissism', 'machiavellianism', 'psychopathy', 'overall'];
        const currentIndex = dimensions.indexOf(currentDimension);
        const nextIndex = (currentIndex + 1) % dimensions.length;
        showDimensionFeedback(dimensions[nextIndex]);
      }
    });
  </script>
</body>
